{
  "name": "Take Paws",
  "version": "1.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "images.png"
  },
  "permissions": ["storage", "history"]
}
